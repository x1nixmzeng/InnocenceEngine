// shadertype=hlsl

Texture2D uni_geometryPassRT0 : register(t0);
Texture2D uni_geometryPassRT1 : register(t1);
Texture2D uni_geometryPassRT2 : register(t2);

SamplerState SampleTypePoint: register(s0);

struct PixelInputType
{
    float4 position : SV_POSITION;
    float2 texcoord : TEXCOORD0;
};

float4 FinalBlendPassPixel(PixelInputType input) : SV_TARGET
{
	float4 textureColor = uni_geometryPassRT2.Sample(SampleTypePoint, input.texcoord);
  return textureColor;
}